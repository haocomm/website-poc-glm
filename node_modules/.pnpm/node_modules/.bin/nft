#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/website-poc-glm/node_modules/.pnpm/@vercel+nft@0.30.3_rollup@4.52.5/node_modules/@vercel/nft/out/node_modules:/workspaces/website-poc-glm/node_modules/.pnpm/@vercel+nft@0.30.3_rollup@4.52.5/node_modules/@vercel/nft/node_modules:/workspaces/website-poc-glm/node_modules/.pnpm/@vercel+nft@0.30.3_rollup@4.52.5/node_modules/@vercel/node_modules:/workspaces/website-poc-glm/node_modules/.pnpm/@vercel+nft@0.30.3_rollup@4.52.5/node_modules:/workspaces/website-poc-glm/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/website-poc-glm/node_modules/.pnpm/@vercel+nft@0.30.3_rollup@4.52.5/node_modules/@vercel/nft/out/node_modules:/workspaces/website-poc-glm/node_modules/.pnpm/@vercel+nft@0.30.3_rollup@4.52.5/node_modules/@vercel/nft/node_modules:/workspaces/website-poc-glm/node_modules/.pnpm/@vercel+nft@0.30.3_rollup@4.52.5/node_modules/@vercel/node_modules:/workspaces/website-poc-glm/node_modules/.pnpm/@vercel+nft@0.30.3_rollup@4.52.5/node_modules:/workspaces/website-poc-glm/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@vercel/nft/out/cli.js" "$@"
else
  exec node  "$basedir/../@vercel/nft/out/cli.js" "$@"
fi
