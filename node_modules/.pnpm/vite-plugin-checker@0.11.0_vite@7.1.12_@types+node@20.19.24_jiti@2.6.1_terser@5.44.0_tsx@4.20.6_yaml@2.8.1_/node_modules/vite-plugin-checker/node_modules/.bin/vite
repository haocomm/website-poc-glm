#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/website-poc-glm/node_modules/.pnpm/vite@7.1.12_@types+node@20.19.24_jiti@2.6.1_terser@5.44.0_tsx@4.20.6_yaml@2.8.1/node_modules/vite/bin/node_modules:/workspaces/website-poc-glm/node_modules/.pnpm/vite@7.1.12_@types+node@20.19.24_jiti@2.6.1_terser@5.44.0_tsx@4.20.6_yaml@2.8.1/node_modules/vite/node_modules:/workspaces/website-poc-glm/node_modules/.pnpm/vite@7.1.12_@types+node@20.19.24_jiti@2.6.1_terser@5.44.0_tsx@4.20.6_yaml@2.8.1/node_modules:/workspaces/website-poc-glm/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/website-poc-glm/node_modules/.pnpm/vite@7.1.12_@types+node@20.19.24_jiti@2.6.1_terser@5.44.0_tsx@4.20.6_yaml@2.8.1/node_modules/vite/bin/node_modules:/workspaces/website-poc-glm/node_modules/.pnpm/vite@7.1.12_@types+node@20.19.24_jiti@2.6.1_terser@5.44.0_tsx@4.20.6_yaml@2.8.1/node_modules/vite/node_modules:/workspaces/website-poc-glm/node_modules/.pnpm/vite@7.1.12_@types+node@20.19.24_jiti@2.6.1_terser@5.44.0_tsx@4.20.6_yaml@2.8.1/node_modules:/workspaces/website-poc-glm/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vite@7.1.12_@types+node@20.19.24_jiti@2.6.1_terser@5.44.0_tsx@4.20.6_yaml@2.8.1/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../vite@7.1.12_@types+node@20.19.24_jiti@2.6.1_terser@5.44.0_tsx@4.20.6_yaml@2.8.1/node_modules/vite/bin/vite.js" "$@"
fi
