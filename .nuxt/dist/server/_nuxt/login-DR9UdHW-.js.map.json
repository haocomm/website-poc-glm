;AAAA,oBACnB,MAAK;AAAA,oBACL,aAAY;AAAA,oBACZ,UAAA;AAAA;;;;cAIJG,YAOa,uBAAA;AAAA,gBAPD,OAAM;AAAA,gBAAW,MAAK;AAAA;iCAChC,MAKE;AAAA,kBALFA,YAKE,mBAAA;AAAA,oBAJS,YAAAH,MAAA,IAAA,EAAK;AAAA,oBAAL,uBAAA,YAAAA,MAAA,IAAA,EAAK,WAAQ;AAAA,oBACtB,MAAK;AAAA,oBACL,aAAY;AAAA,oBACZ,UAAA;AAAA;;;;cAIJG,YASU,oBAAA;AAAA,gBARR,MAAK;AAAA,gBACL,OAAM;AAAA,gBACN,SAAQ;AAAA,gBACR,MAAK;AAAA,gBACJ,SAASH,MAAA,OAAA;AAAA,gBACV,OAAM;AAAA;iCACP,MAED;AAAA,kCAFC,WAED;AAAA;;;;;;;;UAGSA,MAAA,KAAA,GAAK;8EACXA,MAAA,KAAA,CAAK,CAAA,QAAA;AAAA;;;;;;;;;;;;;","names":["_ssrRenderAttrs","_mergeProps","_push","_parent","_unref","_","_scopeId","_createVNode"],"sources":["../../../../pages/auth/login.vue"],"sourcesContent":["<template>\n  <div class=\"min-h-screen flex items-center justify-center bg-gray-50 dark:bg-gray-900\">\n    <div class=\"max-w-md w-full space-y-8 p-8\">\n      <div class=\"text-center\">\n        <h2 class=\"text-3xl font-bold text-gray-900 dark:text-white\">\n          Sign in to your account\n        </h2>\n        <p class=\"mt-2 text-sm text-gray-600 dark:text-gray-400\">\n          Or\n          <NuxtLink to=\"/auth/register\" class=\"text-primary-600 hover:text-primary-500\">\n            create a new account\n          </NuxtLink>\n        </p>\n      </div>\n\n      <UForm\n        :state=\"form\"\n        @submit=\"handleLogin\"\n        class=\"space-y-6\"\n      >\n        <UFormGroup label=\"Email\" name=\"email\">\n          <UInput\n            v-model=\"form.email\"\n            type=\"email\"\n            placeholder=\"Enter your email\"\n            required\n          />\n        </UFormGroup>\n\n        <UFormGroup label=\"Password\" name=\"password\">\n          <UInput\n            v-model=\"form.password\"\n            type=\"password\"\n            placeholder=\"Enter your password\"\n            required\n          />\n        </UFormGroup>\n\n        <UButton\n          type=\"submit\"\n          color=\"primary\"\n          variant=\"solid\"\n          size=\"lg\"\n          :loading=\"loading\"\n          class=\"w-full\"\n        >\n          Sign in\n        </UButton>\n      </UForm>\n\n      <div v-if=\"error\" class=\"text-red-500 text-sm text-center\">\n        {{ error }}\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nconst { $client } = useNuxtApp()\nconst router = useRouter()\n\nconst form = reactive({\n  email: '',\n  password: ''\n})\n\nconst loading = ref(false)\nconst error = ref('')\n\nasync function handleLogin() {\n  loading.value = true\n  error.value = ''\n\n  try {\n    const response = await $fetch('/api/auth/login', {\n      method: 'POST',\n      body: form,\n      baseURL: 'http://localhost:3001'\n    })\n\n    // Store token and user info\n    localStorage.setItem('auth_token', response.token)\n    localStorage.setItem('user', JSON.stringify(response.user))\n\n    // Redirect to chat\n    await router.push('/chat')\n  } catch (err) {\n    error.value = err?.data?.error || 'Login failed'\n  } finally {\n    loading.value = false\n  }\n}\n\n// SEO Meta\nuseHead({\n  title: 'Login - Chat App',\n  meta: [\n    { name: 'description', content: 'Sign in to your Chat App account' }\n  ]\n})\n</script>"],"version":3}